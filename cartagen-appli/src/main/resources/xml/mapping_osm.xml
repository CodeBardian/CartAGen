<mapping>
    <gene-obj-implementation>
    	<name>osm</name>
    	<factory>fr.ign.cogit.cartagen.osm.schema.OSMSchemaFactory</factory>
    	<root-package-name>fr.ign.cogit.cartagen.osm.schema</root-package-name>
    	<root-class>fr.ign.cogit.cartagen.osm.schema.OsmGeneObj</root-class>
    </gene-obj-implementation>
    <matching>
        <shapeName>roads</shapeName>
        <creationMethod>createRoadLine</creationMethod>
        <scaleRef>15000</scaleRef>
        <theme>road</theme>
        <attributes>
            <attribute>
                <shapeAttr>fclass</shapeAttr>
                <key>highway</key>
            </attribute>
            <attribute>
                <shapeAttr>name</shapeAttr>
                <key>name</key>
            </attribute>   
            <attribute>
                <shapeAttr>ref</shapeAttr>
                <key>ref</key>
            </attribute>
            <attribute>
                <shapeAttr>oneway</shapeAttr>
                <key>oneway</key>
            </attribute>
            <attribute>
                <shapeAttr>maxspeed</shapeAttr>
                <key>maxspeed</key>
            </attribute> 
            <attribute>
                <shapeAttr>layer</shapeAttr>
                <key>layer</key>
            </attribute>
            <attribute>
                <shapeAttr>bridge</shapeAttr>
                <key>bridge</key>
            </attribute>
            <attribute>
                <shapeAttr>tunnel</shapeAttr>
                <key>tunnel</key>
            </attribute>        
        </attributes>
        <filter>
        	<key>highway</key>
        	<value>motorway</value>
        	<value>secondary</value>
        	<value>primary</value>
        	<value>tertiary</value>
        	<value>motorway_link</value>
        	<value>living_street</value>
        	<value>pedestrian</value>
        	<value>residential</value>
        	<value>primary_link</value>
        	<value>secondary_link</value>
        	<value>service</value>
        	<value>tertiary_link</value>
        	<value>unclassified</value>
        </filter>
    </matching>
    <matching>
        <shapeName>buildings</shapeName>
        <creationMethod>createBuilding</creationMethod>
        <scaleRef>15000</scaleRef>
        <theme>building</theme>
        <attributes>
            <attribute>
                <shapeAttr>type</shapeAttr>
                <key>building</key>
            </attribute>
            <attribute>
                <shapeAttr>name</shapeAttr>
                <key>name</key>
            </attribute>
        </attributes>
    </matching>
    <matching>
        <shapeName>landuse</shapeName>
        <creationMethod>createSimpleLandUseArea</creationMethod>
        <scaleRef>15000</scaleRef>
        <theme>landuse</theme>
        <attributes>
            <attribute>
            	<shapeAttr>fclass</shapeAttr>
                <key>landuse</key>
                <shapeAttr>name</shapeAttr>
                <key>name</key>
            </attribute>
        </attributes>
    </matching>
    <matching>
        <shapeName>railways</shapeName>
        <creationMethod>createRailwayLine</creationMethod>
        <scaleRef>15000</scaleRef>
        <theme>railway</theme>
        <attributes>
            <attribute>
                <shapeAttr>fclass</shapeAttr>
                <key>railway</key>
                <shapeAttr>name</shapeAttr>
				<key>name</key>
            </attribute>
        </attributes>
    </matching>    
	<matching>
		<shapeName>waterways</shapeName>
		<creationMethod>createWaterLine</creationMethod>
		<scaleRef>15000</scaleRef>
		<theme>hydro</theme>
		<attributes>
			<attribute>
				<shapeAttr>fclass</shapeAttr>
				<key>waterway</key>
				<shapeAttr>width</shapeAttr>
				<key>width</key>
				<shapeAttr>name</shapeAttr>
				<key>name</key>
			</attribute>
		</attributes>
	</matching>
	<matching>
		<shapeName>water_area</shapeName>
		<creationMethod>createWaterArea</creationMethod>
		<scaleRef>15000</scaleRef>
		<theme>hydro</theme>
		<attributes>
			<attribute>
				<shapeAttr>fclass</shapeAttr>
				<key>natural</key>
				<shapeAttr>name</shapeAttr>
				<key>name</key>
			</attribute>
		</attributes>
	</matching>    
    <matching>
        <shapeName>roads_collapsed</shapeName>
        <creationMethod>createRoadLine</creationMethod>
        <scaleRef>15000</scaleRef>
        <theme>road</theme>
        <attributes>
            <attribute>
                <shapeAttr>fclass</shapeAttr>
                <key>highway</key>
            </attribute>
            <attribute>
                <shapeAttr>name</shapeAttr>
                <key>name</key>
            </attribute>         
        </attributes>
    </matching>
    <matching>
        <shapeName>roads_collapsed_2</shapeName>
        <creationMethod>createRoadLine</creationMethod>
        <scaleRef>15000</scaleRef>
        <theme>road</theme>
        <attributes>
            <attribute>
                <shapeAttr>fclass</shapeAttr>
                <key>highway</key>
            </attribute>
            <attribute>
                <shapeAttr>name</shapeAttr>
                <key>name</key>
            </attribute>         
        </attributes>
    </matching>
</mapping>