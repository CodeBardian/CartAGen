<?xml version="1.0" encoding="ISO-8859-1"?>
<cartagen-dataset>
  <name>AzelleData</name>
  <type host="localhost" port="5432" dbName="azelle" user="postgres" password="postgres" schema="public">fr.ign.cogit.cartagen.core.dataset.postgis.PostGISCartAGenDB</type>
  <dataset-type>fr.ign.cogit.cartagen.core.dataset.CartAGenDataSet</dataset-type>
  <scale>25000</scale>
  <classes-list>
    <class>
      <name>data25_routes</name>
      <feature-type>RoadLine</feature-type>
    </class>
    <class>
      <name>data25_bat</name>
      <feature-type>Building</feature-type>
    </class>		
  </classes-list>
  <mapping-file>src/main/resources/xml/mapping_azelle.xml</mapping-file>
  <geneobj-implementation>
    <implementation-name>default</implementation-name>
    <implementation-package>fr.ign.cogit.cartagen.core.defaultschema</implementation-package>
    <implementation-root-class>fr.ign.cogit.cartagen.core.defaultschema.GeneObjDefault</implementation-root-class>
    <implementation-factory>fr.ign.cogit.cartagen.core.defaultschema.DefaultCreationFactory</implementation-factory>
  </geneobj-implementation>
  <persistent>false</persistent>
  <persistent-classes/>
</cartagen-dataset>
